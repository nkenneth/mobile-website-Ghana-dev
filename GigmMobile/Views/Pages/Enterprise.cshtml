<style>
	.double-nav .button-collapse {
		display: none !important;
		position: relative;
		font-size: 1.4rem;
		margin-top: 2px;
		margin-right: 10px;
		margin-left: 10px;
	}

	@@media (min-width: 768px) {
		.modal-dialog {
			width: 800px !important;
			margin: 30px auto;
			-webkit-font-smoothing: antialiased;
		}
		/*.modal.center .modal-dialog {-webkit-transform: translate3d(0,0,0); }




			.modal.in .modal-dialog,
	.modal.fade .modal-dialog {
	-webkit-transform: inherit !important;
	-moz-transform: inherit !important;
	-o-transform: inherit !important;
	transform: inherit !important;
	}*/
	}

	.row-flex2 {
		display: flex;
		justify-content: space-between;
	}


	@@media screen and (max-width: 800px) {
		.row-flex2 {
			display: block;
			justify-content: space-between;
		}

		.row-flex3 {
			display: flex;
			justify-content: space-between;
		}

		.row-flex .item-2 {
			flex-basis: 30%;
		}
	}

	.row-flex2 .col2 {
		flex-basis: 48%;
	}

	.d-flexy {
		display: flex;
		justify-content: space-between;
	}

		.d-flexy .item:nth-child(2) {
			font-size: 1.5em;
			color: white;
			margin-top: 2%;
		}

	.fade {
		opacity: 0;
		-webkit-transition: opacity 0.05s linear;
		-moz-transition: opacity 0.05s linear;
		-ms-transition: opacity 0.05s linear;
		-o-transition: opacity 0.05s linear;
		transition: opacity 0.05s linear;
		background: rgba(0, 0, 0, 0.52);
	}

	.modal-dialog img {
		width: 100%;
	}

	.modal-fade {
		transition: transform .003s ease-out;
	}

	.modal-footer {
		padding: 15px;
		text-align: right;
		background: #FEFEFE;
		border-top: 1px solid #e5e5e5;
	}

	body {
		display: none;
	}

	.row-flex {
		display: flex;
		justify-content: space-between;
	}

		.row-flex .item-2 {
			flex-basis: 10%;
		}

	.add-user {
		background-color: #000000b0 !important;
		color: white !important;
		font-size: .8rem !important;
		padding: .85rem 2.13rem !important;
		margin: 6px !important;
		border-radius: 2px !important;
		border: 0 !important;
		-webkit-transition: .2s ease-out;
		transition: .2s ease-out;
		text-transform: uppercase;
		white-space: normal !important;
		cursor: pointer;
		-webkit-box-shadow: 0 2px 5px 0 rgba(0,0,0,.16), 0 2px 10px 0 rgba(0,0,0,.12);
		box-shadow: 0 2px 5px 0 rgba(0,0,0,.16), 0 2px 10px 0 rgba(0,0,0,.12);
	}


	a:not([href]):not([tabindex]) {
		color: inherit;
		text-decoration: none;
		box-shadow: none;
		background: rgba(255,255,255,0);
		padding-bottom: 0%;
		border: 0;
		border-bottom: 1px solid rgba(0,0,0,.25);
	}

	[type=radio]:checked + label:after {
		border-color: #cb373a;
		background-color: #cb373a;
		z-index: 0;
		-webkit-transform: scale(1.02);
		-ms-transform: scale(1.02);
		transform: scale(1.02);
	}

	.nav-desk {
		display: none;
	}

	@@media screen and (min-width: 800px) {
		.nav-desk {
			display: block;
		}

		.header {
			display: none;
		}

		body {
			background-image: url(../../newmenu/ep-back.jpg) !important;
			background-size: 100% !important;
			background-repeat: no-repeat !important;
		}

		.card-imger {
			display: none;
			position: absolute;
			z-index: 200;
			width: 50% !important;
			margin: auto !important;
		}

		.custom-modal-body {
			position: relative;
			-ms-flex: 1 1 auto;
			flex: 1 1 auto;
			padding: 0px !important;
		}
			/*.custom-modal-body > button {
			position: absolute;
			margin-left: 78.5%;
			margin-top: -9%;
		}*/
			.custom-modal-body > button {
				position: absolute;
				margin-left: -95%;
				margin-top: 46%;
			}

		.modal-backdrop {
			background-color: rgba(0, 0, 0, 0.69);
		}

		#myModal2 > .modal-dialog {
			width: 80% !important;
			max-width: 70% !important;
		}

		.card {
			width: 90%;
			margin: auto;
		}

		.custom-sub-btn {
			width: 40%;
			margin: auto;
			margin-top: 3em;
		}
	}

	.navbar-buttons button {
		position: relative;
		border: none;
		padding: 0.9rem 2.3rem;
		font-size: 1rem;
		text-transform: uppercase;
		cursor: pointer;
		font-weight: 600;
		color: #ed482f;
		border-radius: 50px;
		background-color: #eeecec;
		text-align: center;
	}

	.navbar-brand {
		position: absolute;
	}

	.loader {
		background-color: rgba(0,0,0,.7);
		position: fixed;
		width: 100%;
		display: block;
		height: 100%;
		top: 0;
	}

		.loader > img {
			max-width: 100%;
			max-height: 20%;
			margin: auto;
			margin-top: 20%;
			align-self: center;
		}
</style>


<script>
	//multi step part

	$(document).ready(function () {

		//On INIT hide sexctions
		$(".card3").hide();
		$(".card4").hide();
		$("#submitBtn").hide();
		//hide back button on init
		$("#back").hide();

		//next button
		$("#nexty").click(function () {



			let fName = $("#name").val();
			let lName = $("#lastname").val();
			let number = $("#number").val();
			let email = $("#email").val();
			$(".card1").show();


			if (!fName) {
				swal("Please enter a valid First Name")
                    .then((value) => {
                    	return false;
                    	$(".card1").show();

                    });

				return false;

			}

			else if (!lName) {
				swal("Please enter a valid Last Name")
                    .then((value) => {
                    	return false;
                    	$(".card1").show();

                    });

				return false;

			}

			else if (!email) {
				swal("Please enter a valid Email")
                    .then((value) => {
                    	return false;
                    	$(".card1").show();

                    });

				return false;

			}
			else if (!number) {
				swal("Please enter a valid Phone Number")
					.then((value) => {
						return false;
						$(".card1").show();
					});

				return false;

			}
			else if (fName && lName && email && number ) {
				//Intiate
				$(".card1").hide();
				$(".card2").hide();
				//hide boxes
				$(".card3").fadeIn();
				$(".card4").fadeIn();
				//show boxes
				$(this).hide()
				$("#submitBtn").fadeIn();
				$("#zBacka").attr("href", "#");
				$("#zBacka").attr("display", "block !important");
				$("#zBacka").show();
				if ($(window).width() > 800) {
					$("#back").show();
				}


			}
		});
		$("#zBacka").click(function () {
			let fName = $("#name").val();
			let lName = $("#lastname").val();
			let number = $("#number").val();
			let email = $("#email").val();


			if (fName && lName && email && number) {
				//Intiate
				$(".card1").show();
				$(".card2").fadeIn();
				//hide boxes
				$(".card3").hide();
				$(".card4").hide();
				//show boxes
				$(this).hide()
				$("#submitBtn").hide();
				$("#nexty").show();
				//$("#zBacka").attr("href", "javascript:history.back()");
			}

			else {

				$("#zBacka").attr("href", "javascript:history.back()");

				}
		});



		//back button
		$("#back").click(function () {



			//Intiate
			$(".card1").show();
			$(".card2").fadeIn();
			//hide boxes
			$(".card3").hide();
			$(".card4").hide();
			//show boxes
			$(this).hide()
			$("#submitBtn").hide();
			$("#nexty").show();

		});

	});



</script>